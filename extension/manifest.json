{
    "name": "No metrics",
    "author": "Gaetan Puleo",
    "description": "Remove numbers to focus on content",
    "version": "0.0.4",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["*://twitter.com/*"],
            "css": ["twitter.css"]
        },
        {
            "matches": ["*://www.youtube.com/*"],
            "css": ["youtube.css"]
        }
    ],
    "icons": { 
        "16": "icon-16.png",
        "32": "icon-32s.png",
        "48": "icon-48.png",
       "128": "icon-128.png" 
    }
}