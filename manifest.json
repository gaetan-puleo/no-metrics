{
    "name": "No metrics",
    "version": "0.0.2",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["*://twitter.com/*"],
            "css": ["twitter.css"]
        },
        {
            "matches": ["*://www.youtube.com/*"],
            "css": ["youtube.css"]
        }
    ]
}